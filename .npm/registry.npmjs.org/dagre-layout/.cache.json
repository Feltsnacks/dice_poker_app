":"^22.4.2","karma":"^2.0.0","karma-chrome-launcher":"^2.2.0","karma-firefox-launcher":"^1.1.0","karma-mocha":"^1.3.0","karma-safari-launcher":"^1.0.0","mocha":"^5.0.4","sprintf":"^0.1.5","standard":"^11.0.1","webpack":"^4.1.1","webpack-cli":"^2.0.12","webpack-node-externals":"^1.6.0","yarn-upgrade-all":"^0.3.0"},"repository":{"type":"git","url":"git+https://github.com/tylingsoft/dagre-layout.git"},"license":"MIT","files":["dist/","lib/","index.js"],"standard":{"ignore":["dist/**/*.js","coverage/**/*.js"]},"jest":{"testRegex":"test/.+?-test\\.js","testPathIgnorePatterns":["test/bundle-test\\.js"]},"gitHead":"34a8bd98e4b953cf4137dd9422313d5c609e9f53","bugs":{"url":"https://github.com/tylingsoft/dagre-layout/issues"},"homepage":"https://github.com/tylingsoft/dagre-layout#readme","_id":"dagre-layout@0.8.8","_npmVersion":"5.7.1","_nodeVersion":"9.6.1","_npmUser":{"name":"tylerlong","email":"tyler4long@gmail.com"},"dist":{"integrity":"sha512-ZNV15T9za7X+fV8Z07IZquUKugCxm5owoiPPxfEx6OJRD331nkiIaF3vSt0JEY5FkrY0KfRQxcpQ3SpXB7pLPQ==","shasum":"9b6792f24229f402441c14162c1049e3f261f6d9","tarball":"https://registry.npmjs.org/dagre-layout/-/dagre-layout-0.8.8.tgz","fileCount":34,"unpackedSize":1222604},"maintainers":[{"name":"tylerlong","email":"tyler4long@gmail.com"}],"directories":{},"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/dagre-layout_0.8.8_1521551383268_0.58459689006835"}}},"readme":"# dagre-layout - Graph layout for JavaScript\n\n[![Build Status](https://secure.travis-ci.org/tylingsoft/dagre-layout.png?branch=master)](http://travis-ci.org/tylingsoft/dagre-layout)\n[![Coverage Status](https://coveralls.io/repos/github/tylingsoft/dagre-layout/badge.svg?branch=master)](https://coveralls.io/github/tylingsoft/dagre-layout?branch=master)\n\nThis project is an out-of-box replacement for [dagre](https://github.com/dagrejs/dagre).\n\nThis project isn't built from scratch. It's based on the original [dagre](https://github.com/dagrejs/dagre) project.\n\nDagre is a JavaScript library that makes it easy to lay out directed graphs on the client-side.\n\nFor more details, including examples and configuration options, please see our [wiki](https://github.com/dagrejs/dagre/wiki).\n\n\n## Changes compared to dagrejs/dagre\n\n- Upgrade all the dependencies (loadash 3 => 4)\n- Yarn instead of NPM\n- Get rid of PhantomJS\n- ES6 instead of ES5\n- Use webpack instead of browserify\n- Use JavaScript Standard Style instead of JSHint\n- Use Jest for testing\n- Add test coverage report\n- Remove Bower support\n- Use 0 instead of `Number.NEGATIVE_INFINITY`\n- Git ignore auto-generated code\n\n\n## Setup\n\n```\nyarn install\n```\n\n\n## Build\n\n```\nyarn build\n```\n\n\n## Test\n\n```\nyarn test\n```\n\n\n## License\n\ndagre-layout is licensed under the terms of the MIT License. See the LICENSE file for details.\n","maintainers":[{"name":"tylerlong","email":"tyler4long@gmail.com"}],"time":{"modified":"2018-03-20T13:09:43.644Z","created":"2017-09-05T14:46:14.881Z","0.7.5":"2017-09-05T14:46:14.881Z","0.7.6":"2017-09-05T15:16:02.752Z","0.7.7":"2017-09-05T15:50:31.380Z","0.7.8":"2017-09-06T02:44:32.954Z","0.7.9":"2017-09-08T12:15:05.706Z","0.7.10":"2017-09-08T12:34:12.330Z","0.8.0":"2017-09-08T14:06:24.238Z","0.8.2":"2018-02-11T10:34:25.836Z","0.8.4":"2018-02-12T05:25:17.193Z","0.8.5":"2018-03-01T01:51:04.804Z","0.8.6":"2018-03-03T09:58:05.231Z","0.8.7":"2018-03-20T06:35:33.978Z","0.8.8":"2018-03-20T13:09:43.370Z"},"homepage":"https://github.com/tylingsoft/dagre-layout#readme","keywords":["graph","layout","dagre"],"repository":{"type":"git","url":"git+https://github.com/tylingsoft/dagre-layout.git"},"author":{"name":"Tyler Long","email":"tyler4long@gmail.com"},"bugs":{"url":"https://github.com/tylingsoft/dagre-layout/issues"},"license":"MIT","readmeFilename":"README.md","_attachments":{},"_etag":"W/\"2001989a9ad7d7ec33efc0830fd6c149\"","_lastModified":"Sat, 26 May 2018 21:40:28 GMT"}